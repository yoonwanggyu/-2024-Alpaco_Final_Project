{"\uccb5\uccb5 \uccb4\ud06c\uce74\ub4dc(\ub9c8\ub8e8\ub294\uac15\uc950)": {"card_describe": "\ub9c8\ub8e8\uc640 \ud568\uaed8 \ud61c\ud0dd\ub3c4 \uccb5\uccb5!", "\ub300\uc911\uad50\ud1b5": "\ubc84\uc2a4/\uc9c0\ud558\ucca0\n2~4\ucc9c\uc6d0 \uccad\uad6c\ud560\uc778", "\ud3b8\uc758\uc810": "CU\ud3b8\uc758\uc810\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778", "\ucee4\ud53c": "\uc2a4\ud0c0\ubc85\uc2a4(\uc0ac\uc774\ub80c\uc624\ub354\ud3ec\ud568)\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778", "\uc601\ud654": "CGV\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778", "\uc1fc\ud551": "\ud150\ubc14\uc774\ud150\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778", "\uc628\ub77c\uc778\uac04\ud3b8\uacb0\uc81c": "\ub124\uc774\ubc84/\uce74\uce74\uc624/SSG\ud398\uc774\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778", "\ubdf0\ud2f0": "\uc62c\ub9ac\ube0c\uc601/\uc548\uacbd(\ub80c\uc988)\uc810\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778", "\uc628\ub77c\uc778\uc11c\uc810": "YES24/\uad50\ubcf4\ubb38\uace0\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778", "\ubb38\ud654": "\uc778\ud130\ud30c\ud06c\ud2f0\ucf13\n2~4\ucc9c\uc6d0 \ud658\uae09\ud560\uc778"}, "\ud2b8\ub798\ube14\ub7ec\uc2a4 \uccb4\ud06c\uce74\ub4dc": {"card_describe": "\uc5ec\ud589\uc774 \uc77c\uc0c1\uc778 \ud504\ub85c\uc5ec\ud589\ub7ec\ub77c\uba74", "\ud574\uc678 \ud61c\ud0dd": "\ud574\uc678 \uac00\ub9f9\uc810\n\uc774\uc6a9 \uc218\uc218\ub8cc \uba74\uc81c"}, "\ud2b8\ub798\ube14\ub7ec\uc2a4 \uccb4\ud06c\uce74\ub4dc(\ud1a0\uc2ec\uc774)": {"card_describe": "\uc5ec\ud589\uc774 \uc77c\uc0c1\uc778 \ud504\ub85c\uc5ec\ud589\ub7ec\ub77c\uba74", "\ud574\uc678 \ud61c\ud0dd": "\ud574\uc678 ATM\n\uc774\uc6a9 \uc218\uc218\ub8cc \uba74\uc81c", "\uad6d\ub0b4 \ud61c\ud0dd": "\ud478\ub518\ucf54 \uc120\uc815\n\uc804\uad6d\ub9db\uc9d1 5,000\uc6d0 \ud560\uc778"}, "\ud1a0\uc2ec\uc774 \uccb5\uccb5 \uccb4\ud06c\uce74\ub4dc": {"card_describe": "\uc904 \uc11c\uc11c \ubc1c\uae09\ud558\ub294 \ud61c\ud0dd \ub9db\uc9d1!", "\ub300\uc911\uad50\ud1b5": "\ubc84\uc2a4/\uc9c0\ud558\ucca0\n2~4\ucc9c\uc6d